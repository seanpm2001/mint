---=== /__mint__/index.js ===---
import {
  lazyComponent as F,
  createElement as E,
  lazy as B
} from "./runtime.js";

export const
  a = () => {
    return ``
  },
  b = () => {
    return a()
  },
  A = B(`./1.js`),
  C = B(`./2.js`),
  D = () => {
    return E(`div`, {}, [
      E(F, {
        key: `First`,
        x: A,
        p: {},
        c: []
      }),
      E(F, {
        key: `Second`,
        x: C,
        p: {},
        c: []
      }),
      b()
    ])
  };

---=== /__mint__/1.js ===---
import { createElement as B } from "./runtime.js";

import { b as a } from "./index.js";

export const A = () => {
  return B(`div`, {}, [
    `First`,
    a()
  ])
};

export default A;

---=== /__mint__/2.js ===---
import { createElement as B } from "./runtime.js";

import { b as a } from "./index.js";

export const A = () => {
  return B(`div`, {}, [
    `Second`,
    a()
  ])
};

export default A;
