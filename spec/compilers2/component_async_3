module Test {
  fun blah {
    halb()
  }

  fun halb() {
    ""
  }
}

async component First {
  fun render : Html {
    <div>
      "First"
      Test.blah()
    </div>
  }
}

async component Second {
  fun render : Html {
    <div>
      "Second"
      Test.blah()
    </div>
  }
}

component Main {
  fun render : Html {
    <div>
      <First/>
      <Second/>
      Test.blah()
    </div>
  }
}
