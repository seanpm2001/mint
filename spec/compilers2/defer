module Test {
  const TEST = defer { test() + B.c() }

  fun test {
    "Hello!"
  }
}

module B {
  fun c {
    "Blah"
  }
}

component Main {
  fun componentDidMount : Promise(String) {
    let a = await Test.TEST

    a
  }

  fun render : Html {
    <div>
      B.c()
    </div>
  }
}
--------------------------------------------------------------------------------
---=== /__mint__/index.js ===---
import {
  createElement as D,
  useEffect as B,
  load as C
} from "/__mint__/runtime.js";

export const
  a = () => {
    return `Blah`
  },
  b = `/__mint__/1.js`,
  A = () => {
    B(() => {
      (async () => {
        const c = await C(b);
        return c
      })()
    }, []);
    return D(`div`, {}, [a()])
  };

---=== /__mint__/1.js ===---
import { a as c } from "/__mint__/index.js";

export const
  a = () => {
    return `Hello!`
  },
  b = a() + c();

export default b;
