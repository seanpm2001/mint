component Test {
  property test : Html

  fun render : Html {
    test
  }
}

component Main {
  fun render : Html {
    <Test test=<{ "Hello" }>/>
  }
}
--------------------------------------------------------------------------------
import { createElement as C } from "./runtime.js";

export const
  A = ({
    a
  }) => {
    return a
  },
  B = () => {
    return C(A, {
      a: `Hello`
    })
  };
