enum Result(error, value) {
  Err(error)
  Ok(value)
}

enum Maybe(value) {
  Nothing
  Just(value)
}

component Main {
  fun render : String {
    Maybe::Nothing or "Hello"
    ""
  }
}
--------------------------------------------------------------------------------
import {
  variant as B,
  or as G
} from "/__mint__/runtime.js";

export const
  A = B(0),
  C = B(1),
  D = B(1),
  E = B(1),
  F = () => {
    G(A, C, new A(), `Hello`);
    return ``
  };
