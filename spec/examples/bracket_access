------------------------------------------------bracket_access_index_not_number
component Main {
  fun test : Maybe(String) {
    [
      "Hello",
      "Blah",
      "Joe"
    ]["asd"]
  }

  fun render : Html {
    test()

    <div/>
  }
}
----------------------------------------------------bracket_access_not_an_array
component Main {
  fun test : Maybe(String) {
    {}[0]
  }

  fun render : Html {
    test()

    <div/>
  }
}
---------------------------------------------------bracket_access_invalid_tuple
component Main {
  fun test : Maybe(String) {
    {"Hello", ""}[2]
  }

  fun render : Html {
    test()

    <div/>
  }
}
-------------------------------------------------------------------------------
